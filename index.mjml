<mjml>
  <mj-head>
    <mj-breakpoint width="400px" />
    <mj-title>Father’s day is coming soon</mj-title>
    <mj-style>
      .top-section-btn:hover { background: #fff !important; color: #000
      !important; } @media only screen and (min-width: 400px) { .treat-yourself
      { font-size: 32px; line-height: 34px; } .treat-yourself-img { width: 100%
      !important; } .treat-yourself-title {font-size: 20px !important;
      line-height: 145% !important; } .treat-yourself-p {font-size: 16px
      !important; line-height: 145% !important; margin: 8px 0 10px 0; }
      .treat-yourself-btn { font-size: 14px; padding-top: 15px !important;} }
    </mj-style>

    <mj-style inline="inline">
      .top-section-btn { color: #ffffff; text-decoration: none; border: 2px
      solid black; line-height: 20px; background: #000; font-size: 17px;
      padding: 1em 3em; margin-top: 20px }
    </mj-style>
  </mj-head>
